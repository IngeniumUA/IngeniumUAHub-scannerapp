<ScanScreen>:
    ScanAnalyze:
        size_hint_y: 0.8
        size_hint_x: 0.98
        pos_hint: {"x":0.01, "y":0.1}
        id:preview
        extracted_data: root.got_result

    MDLabel:
        id: event_empty
		size_hint_y: None
		height:'30dp'
		size_hint_x: 0.7
        pos_hint: {"x":0.15, "y":0.5}
		halign: 'center'
		color: 1,0,0,1
		text: "Selecteer eerst een evenement"
		opacity: 1
		font_size: 100
		font_name: 'app/assets/D-DIN.otf'

    Button:
        size_hint_y: 0.08
        size_hint_x: 0.98
        pos_hint: {"x":0.01, "y":0}
        text: "Terug"
        font_name: 'app/assets/D-DIN.otf'
        color: 1,1,1,1
        background_normal: 'app/assets/buttonnormal.png'
        background_down: 'app/assets/buttondown.png'
        on_release:
            root.manager.transition.direction = "right"
            root.manager.current = "login"
